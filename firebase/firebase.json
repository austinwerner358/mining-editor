{
  "hosting": {
    "public": "public",
    "redirects": [
      {
        "source": "/firebase/*",
        "destination": "https://firebase.google.com",
        "type": 302
      },
      {
        "source": "/local",
        "destination": "/?local=true",
        "type": 301
      }
    ],
    "rewrites" : [ {
      "source": "/control",
      "destination": "/controls.html"
    } ],
    "cleanUrls": true,
    "trailingSlash": false
  }
}
